{
  "name": "frolint",
  "description": "git commit hook linter",
  "version": "0.7.11",
  "author": "Yuki Yamada <yamada@wantedly.com>",
  "bin": "lib/index.js",
  "dependencies": {
    "@yamadayuki/ogh": "^1.1.0",
    "arg": "^4.1.0",
    "chalk": "^2.4.2",
    "clipanion": "^2.1.3",
    "command-exists": "^1.2.8",
    "cosmiconfig": "^5.2.1",
    "eslint": "^6.0.1",
    "eslint-config-wantedly": "^0.7.11",
    "eslint-config-wantedly-typescript": "^0.7.11",
    "execa": "^3.1.0",
    "prettier": "^1.19.1",
    "prettier-config-wantedly": "^0.7.11",
    "resolve": "^1.10.0"
  },
  "homepage": "https://github.com/wantedly/frolint",
  "keywords": [
    "eslint",
    "githooks"
  ],
  "license": "MIT",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/wantedly/frolint/tree/master/packages/frolint",
  "scripts": {
    "postinstall": "node lib/index.js install",
    "preuninstall": "node lib/index.js uninstall",
    "test": "jest",
    "build": "tsc -b .",
    "build:watch": "tsc -b . --watch"
  },
  "devDependencies": {
    "@types/command-exists": "^1.2.0",
    "@types/cosmiconfig": "^5.0.3",
    "@types/eslint": "^6.1.3",
    "@types/mock-fs": "^4.10.0",
    "@types/prettier": "^1.18.3",
    "@types/resolve": "^0.0.8"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testEnvironment": "node"
  }
}
