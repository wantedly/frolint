{
  "name": "eslint-plugin-wantedly",
  "description": "Opinionated ESLint plugin for Wantedly",
  "version": "3.2.2",
  "author": "Yuki Yamada <yamada@wantedly.com>",
  "homepage": "https://github.com/wantedly/frolint",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/wantedly/frolint/tree/master/packages/eslint-plugin-wantedly",
  "scripts": {
    "test": "jest",
    "build": "tsc -b .",
    "build:watch": "tsc -b . --watch"
  },
  "dependencies": {
    "camel-case": "^4.1.2",
    "pascal-case": "^3.1.2",
    "snake-case": "^3.0.4"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "graphql": "^15.6.0"
  },
  "peerDependencies": {
    "eslint": "^6.0.0 || ^7.0.0 || ^8.0.0",
    "graphql": "^14.6.0 || ^15.0.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.json"
        }
      ]
    },
    "testEnvironment": "node"
  }
}
